@model LandlordNavBarViewModel

<nav>
    <div class="nav-wrapper">
        <a class="brand-logo"></a>
        <a href="#" data-activates="mobile-demo" class="button-collapse"><i class="material-icons">menu</i></a>
        <ul class="right hide-on-med-and-down">
            <li><a asp-controller="landlord" asp-action="index">Home</a></li>
            <li><a asp-controller="landlord" asp-action="maintenance" asp-route-id="@Model.LandlordId">Maintenance</a></li>
            <li><a class="dropdown-button" href="#!" data-activates="property-list-desktop">Payments<i class="material-icons right">arrow_drop_down</i></a></li>
        </ul>
        <ul class="side-nav" id="mobile-demo">
            <li><a asp-controller="landlord" asp-action="index">Home</a></li>
            <li><a asp-controller="landlord" asp-action="maintenance" asp-route-id="@Model.LandlordId">Maintenance</a></li>
            <li><a class="dropdown-button" href="#!" data-activates="property-list-mobile">Payments<i class="material-icons right">arrow_drop_down</i></a></li>
        </ul>

    </div>
</nav>

<ul id="property-list-desktop" class="dropdown-content">
    @foreach(var p in Model.Properties)
    {
        <li><a asp-controller="landlord" asp-action="payment" asp-route-id="@p.Id">@p.Address</a></li>

    }
</ul>

<ul id="property-list-mobile" class="dropdown-content">
    @foreach (var p in Model.Properties)
    {
        <li><a asp-controller="landlord" asp-action="payment" asp-route-id="@p.Id">@p.Address</a></li>

    }
</ul>