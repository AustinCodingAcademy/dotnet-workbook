@using Project1.Models;
@model Product[]

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <ul>
        @foreach (var p in Model)
        {
            if (p != null)
            {
            <li>
                @p.Name - @p.Price
            </li>
            } 
        }
    </ul>
</body>
</html>
